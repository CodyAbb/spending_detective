<form action="/tags" method="POST">
  <label for="type-name">Tag Name:</label>
  <input type="text" name="type" id="type-name" placeholder="Enter Here">
   <input type="submit"value="Add Tag">
</form>

<ul>
  <% for tag in @tags %>
  <li>
    <%= tag.type %>
    <a href="/tags/<%=tag.id%>/delete">Delete</a>
    <a href="/tags/<%=tag.id%>/edit">Edit</a>
  </li>
  <% end %>
</ul>
